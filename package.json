{
  "name": "@neokekeke/k-ui",
  "version": "0.0.2",
  "description": "k-ui componets",
  "private": false,
  "main": "./lib/k-ui.js",
  "files": [
    "lib",
    "src/components"
  ],
  "scripts": {
    "build-dev": "rimraf dist && cross-env NODE_ENV=development webpack --config ./build/webpack.config.js --progress --profile --colors --display-error-details",
    "build-test": "rimraf dist && cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --progress --profile --colors --display-error-details",
    "build-prod": "rimraf dist && cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --progress --profile --colors --display-error-details",
    "build-prepub": "rimraf dist && cross-env NODE_ENV=production webpack --config ./build/webpack.config.publish.js --progress --profile --colors --display-error-details",
    "analyz": "rimraf dist && cross-env NODE_ENV=production webpack --config ./build/webpack.config.analyz.js --progress --profile --colors --display-error-details",
    "start-webpack-dev-server": "webpack-dev-server --open",
    "start-express": "cross-env NODE_ENV=development node server.js",
    "rimraf": "rimraf dist",
    "publish:prod": "npm version patch && npm run build-prepub && npm publish --access=public"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "author": "https://github.com/Neokekeke",
  "license": "ISC",
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "open": "^8.2.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.1.1",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "element-ui": "^2.15.2",
    "ramda": "^0.27.1",
    "vue": "^2.6.12"
  }
}
